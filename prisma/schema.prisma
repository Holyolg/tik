generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Genproject {
  id          String  @id @default(cuid())
  rating      String
  img         String
  title       String
  subtitle    String?
  category    String
  type        String
  square      String?
  location    String?
  partners    String?
  customer    String?
  stage       String?
  date        String?
  status      String?
  description String?
  imgcontent  String?
}
